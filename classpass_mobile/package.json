  "scripts": {
    "run-android": "react-native run-android",
    "a": "yarn run-android",
    "run-iphone-14": "react-native run-ios --simulator=\"iPhone 14 Pro\"",
    "i": "yarn run-iphone-14",
    "ix": "react-native run-ios --device \"Aliakseiâ€™s iPhone\"",
    "run-iphone-se": "react-native run-ios",
