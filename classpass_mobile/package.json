    "start": "react-native start",
    "clear-cache": "react-native start --reset-cache",
    "clear-npm-cache": "npm start -- --reset-cache",
    "lint": "eslint 'src/**/*'",
    "test": "jest",
    "list-ios-devices": "xcrun simctl list devices",
    "pod": "cd ios && pod install",
    "pod-update": "cd ios && pod install --repo-update",
    "clean-pod": "cd ios && pod cache clean --all && pod install && pod update",
    "clean-y": "yarn cache clean && rm -rf node_modules && yarn",
    "clean-a": "cd android && ./gradlew clean",
    "clean-i": "cd ios && rm -rf build && xcodebuild clean",
    "clean": "yarn clean-y && yarn clean-pod && yarn pod-update && yarn clean-a && yarn clean-i",
    "apk-file": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug && open -R app/build/outputs/apk/debug/app-debug.apk",
    "app-file": "react-native run-ios --configuration=Release && open /Users/aliaksei_dzemyanovich/Library/Developer/Xcode/DerivedData/",
    "pod-hermes": "cd ios && pod update hermes-engine --no-repo-update"
  },
